@page
@model BallotPalette.Pages.AboutModel
@{
    ViewData["Title"] = "About";
}

<h1>About</h1>

<table class="table">

    @foreach(var user in Model.Users)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Username</td>
            <td>@user.Password</td>
            <td>@user.TeamId</td>
        </tr>
    }

</table>



<div>
    @Model.Message
</div>
